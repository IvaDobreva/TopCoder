      <script type="text/ng-template" id="publisherSelectActivity.html">
      <div class="modal-container">
         <div class="titles">
            <h2 class="title-txt" ng-hide="isLoading || !activities.length">Please, select activity.</h2>
         </div>
         <div class="sure-boxs activity-select-list">
            <div class="mains">
               <div class="text-center" ng-show="isLoading">
                 <br/>
                 <div>
                     <img src="/img/loading32.gif" />
                 </div>
               </div>
               <ul ng-hide="isLoading">
                  <li ng-repeat="activity in activities track by activity.id">
                     <a href="javascript:;" ng-click="selectActivity(activity)">{{activity.title}}</a>
                  </li>
                  <li ng-if="!activities.length">
                     <span>Sorry. You have no activities available for registration.&nbsp;</span><span><a href="javascript:;" ng-click="selectActivity()">Close</a></span>
                  </li>
               </ul>
            </div>
            <!-- end .mains -->
            <div class="btn-row">
            </div>
            <!-- end .btn-row -->
         </div>
      </div>
      <!-- end .modal-alert -->
      </script>
      
