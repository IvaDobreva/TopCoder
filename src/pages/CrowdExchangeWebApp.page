               <a href="javascript:;" class="blue-border-btn cancel-btn no-btn modal-window-cancel">NO</a>
               <a href="javascript:;" class="blue-btn width105 radius6 text-center yes-btn modal-window-cancel"><span><span>Yes</span></span></a>
            </div>
            <!-- end .btn-row -->
         </div>
      </div>
      <!-- end .modal-alert -->
      <div class="modal-container modal-alert modal-create-story-steps-cancel hide">
         <div class="titles">
            <a href="javascript:;" class="close-btn modal-window-close">Close</a> 
            <h2 class="title-txt">Alert</h2>
         </div>
         <div class="sure-boxs">
            <div class="mains">
               <p>Are you sure to cancel?</p>
            </div>
            <!-- end .mains -->
            <div class="btn-row">
               <a href="javascript:;" class="blue-border-btn cancel-btn no-btn modal-window-cancel">NO</a>
               <a href="publisher-dashboard.html" class="blue-btn width105 radius6 text-center yes-btn"><span><span>Yes</span></span></a>
            </div>
            <!-- end .btn-row -->
         </div>
      </div>
      <!-- end .modal-alert -->
      <script type="text/ng-template" id="assignMember.html">
      <div class="modal-container modal-alert modal-assign-member">
